{
    "name": "mishbah/diffbot_client",
    "type": "library",
    "description": "A PHP wrapper for using Diffbot's API",
    "keywords": [
        "mishbah",
        "api",
        "client",
        "diffbot"
    ],
    "homepage": "https://github.com/mishbah/diffbot_client",
    "license": "MIT",
    "authors": [
        {
            "name": "Mishbahul Qadri",
            "email": "misbah.qadri@gmail.com",
            "homepage": "http://google.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Mishbah\\Diffbot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mishbah\\Diffbot\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
